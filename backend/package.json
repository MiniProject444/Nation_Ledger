{
  "name": "nationledger-backend",
  "version": "1.0.0",
  "description": "Backend for NationLedger - A blockchain-based document management system",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "tsc",
    "test": "jest",
    "compile": "hardhat compile",
    "deploy": "hardhat run scripts/deploy.ts --network sepolia"
  },
  "keywords": [
    "blockchain",
    "ethereum",
    "ipfs",
    "supabase",
    "document-management"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-waffle": "^2.0.6",
    "@pinata/sdk": "^2.1.0",
    "@supabase/supabase-js": "^2.39.3",
    "@tanstack/react-query": "^5.74.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "^5.7.2",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "hardhat": "^2.19.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
